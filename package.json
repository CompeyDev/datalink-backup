{
  "name": "@datalinkhq/datalink",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "git submodule update --init --remote",
    "update": "ncu",
    "upgrade": "ncu -u",
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "pnpm generate-schema && next build",
    "build:turbo": "pnpm generate-schema && next build --turbo",
    "generate-schema:proxy": "prisma generate --data-proxy",
    "generate-schema": "prisma generate",
    "push-schema": "prisma db push",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "export:turbo": "next export --turbo",
    "vercel-build": "pnpm build && ./vercel.sh"
  },
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "chart.js": "3.9.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "next": "^13.0.0",
    "prisma": "^4.5.0",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/core": "7.19.0",
    "@babel/preset-env": "7.19.0",
    "@faker-js/faker": "7.5.0",
    "@sentry/nextjs": "^7.17.4",
    "@types/glob": "8.0.0",
    "@types/jsonwebtoken": "8.5.9",
    "@types/lodash": "4.14.184",
    "@types/node": "18.7.15",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/uuid": "8.3.4",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "npm-check-updates": "16.1.0",
    "react": "18.2.0",
    "ts-node": "10.9.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "webpack": "5.74.0"
  }
